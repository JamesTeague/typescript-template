{
  "name": "place-package-name-here",
  "version": "0.0.1",
  "description": "template for setting up project",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git-url-goes-here"
  },
  "homepage": "git-url-goes-here#readme",
  "dependencies": {},
  "main": "index.js",
  "module": "",
  "files": [],
  "types": "",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "prettier": "prettier --write 'src/**/*.{js,ts}'",
    "prettier:check": "prettier --check 'src/**/*.{js,ts}'",
    "preversion": "npm run test",
    "version": "",
    "postversion": "",
    "prepublishOnly": "npm run test && npm run build",
    "prerelease": "git checkout master && git pull origin master && npm i && git add .",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version -a --no-verify",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "test": "jest --passWithNoTests"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "git-url-goes-here/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.3",
    "babel-jest": "^26.0.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^1.32.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^6.2.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
